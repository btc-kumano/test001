                      <Sequence.Variables>
                        <Variable x:TypeArguments="x:String" Name="stTodayNow" />
                        <Variable x:TypeArguments="ui:Image" Name="imgScreenshot" />
                      </Sequence.Variables>
                      <Assign DisplayName="[Assign]現在時刻を変数に格納" sap2010:WorkflowViewState.IdRef="Assign_2">
